<mapper namespace="com.example.demo.domain.artifact.dao.userDAO"> <!-- DAO와 연결 -->
<select id="selectIdName"  resultType="com.example.demo.domain.artifact.dto.userDTO" parameterType="com.example.demo.domain.artifact.dto.userDTO">
SELECT * FROM ajaxTable
where id=#{id}
</select>

<select id="selectone"  resultType="com.example.demo.domain.artifact.dto.userDTO" parameterType="com.example.demo.domain.artifact.dto.userDTO">
SELECT * FROM ajaxTable
where id=#{id}
</select>

<select id="ajax"  resultType="com.example.demo.domain.artifact.dto.userDTO" parameterType="com.example.demo.domain.artifact.dto.userDTO">
SELECT name AS name,
id AS id
FROM ajaxTable
where id=#{id} 
AND name=#{name}
</select>





</mapper>